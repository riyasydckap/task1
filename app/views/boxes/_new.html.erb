
<%= form_for (@box), html: {id: "box_form"}, remote: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :sku_prefix %>
  <%= f.text_field :sku_prefix %>
  <br>
  <%= f.label :sku_postfix %>
  <%= f.text_field :sku_postfix %>
  <br>
  <%= f.submit "Create" %>
<% end %>

